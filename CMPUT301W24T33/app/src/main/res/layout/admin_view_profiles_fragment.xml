<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/admin_view_profiles_fragment"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F4EFEF"
    android:clickable="true"
    android:elevation="2dp"
    android:focusable="true"
    tools:context=".organizerFragments.NotificationsOrganizer">

    <RelativeLayout
        android:id="@+id/profile_back_actionbar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <include layout="@layout/actionbar_general" />
    </RelativeLayout>

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/profiles_admin"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@id/profile_back_actionbar"
        android:layout_marginTop="0dp" />


</RelativeLayout>